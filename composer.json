{
    "name": "jeckel-lab/advanced-types",
    "description": "Advanced PHP types for DDD projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Mercier-Rojas",
            "email": "julien@jeckel-lab.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "marc-mabe/php-enum": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "JeckelLab\\Types\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpmd/phpmd": "^2.7",
        "phpstan/phpstan": "^0.11.19",
        "doctrine/dbal": "^2.10"
    },
    "suggest": {
        "doctrine/dbal": "Integrate Value Object as doctrine types"
    }
}
